		"cover": "collection",
		"label": "Nom",
		"scheme" : "http://xmlns.com/foaf/spec/#term_lastName",
		"type" : "https://www.w3.org/TR/xmlschema-2/#string",
		"content<": {
			"get#1" : "director",
			"parseCSV" : " ",
			"last" : true
		}
	},
	"c9": {
		"cover": "collection",
		"label": "Updated",
		"scheme": "http://purl.org/dc/terms/modified",
